<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="_5S_QA_Client.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <userSettings>
    <_5S_QA_Client.Properties.Settings>
      <setting name="Username" serializeAs="String">
        <value />
      </setting>
      <setting name="Password" serializeAs="String">
        <value />
      </setting>
      <setting name="RememberPassword" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="limitLogin" serializeAs="String">
        <value>50</value>
      </setting>
      <setting name="IsVertical" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="IsSameTool" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="Language" serializeAs="String">
        <value>rbEN</value>
      </setting>
      <setting name="Camera" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="IsShowChart" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="Headers" serializeAs="String">
        <value>1;2;3;4;5;6;9;10;11;12;13;14;15;16</value>
      </setting>
      <setting name="CompleteHeaders" serializeAs="String">
        <value>1;2;3;4;5;6;9;10;11;12;13;14;15;16</value>
      </setting>
    </_5S_QA_Client.Properties.Settings>
  </userSettings>
  <connectionStrings>
    <!--Cho đường dẫn API-->
    <add name="APIHost" connectionString="http://5SQA-SERVER"/>
    <!--Tên của client-->
    <add name="MachineName" connectionString="Tablet 1" />
    <!--Lọc kích thước và hiển thị sau dấu thập phân-->
    <add name="Filter" connectionString="" />
    <!--Số lượng hiển thị của biểu đồ-->
    <add name="ChartNumber" connectionString="35" />
    <!--Giới hạn % nhập kết quả-->
    <add name="Deviation" connectionString="5" />
    <!--Cho cập nhập phiên bản phần mềm-->
    <add name="DirUpdate" connectionString="\\5SQA-SERVER\5SQA_UPDATES" />
    <!--Khoảng thời gian bắt đầu lấy mẫu (07:00-08:00)-->
    <add name="TimeSample" connectionString="07:00-08:00" />
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="ICSharpCode.SharpZipLib" publicKeyToken="1b03e6acf1164f73" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.3.3.11" newVersion="1.3.3.11" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>